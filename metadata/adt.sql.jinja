SELECT
    "Oazis" AS metadata_id,
    "OMOPCDM5.4" AS metadata_concept_id,
    "ADMINISTRATION_RECORD" AS metadata_type_concept_id,
    "ADT: Oazis" AS name, 
    "Many tables contain data from our ADT system, Oazis. Of note: all Conditions with ICD source coding come from our ADT." AS value_as_string,
    CAST(NULL AS STRING) AS value_as_concept_id,
    CAST(NULL AS FLOAT64) AS value_as_number,
    CURRENT_DATE() AS metadata_date,
    CURRENT_DATETIME() AS metadata_datetime